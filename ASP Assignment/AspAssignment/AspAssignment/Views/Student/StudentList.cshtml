@model List<AspAssignment.Context.tbl_Student>
@{
    ViewBag.Title = "StudentList";
}
<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Family Name</th>
        <th>Address</th>
        <th>City</th>
        <th>Zip code</th>
        <th>Phone</th>
        <th>Email Id</th>
        <th>Action</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Family_Name</td>
                    <td>@item.Address</td>
                    <td>@item.City</td>
                    <td>@item.Zip_code</td>
                    <td>@item.Phone</td>
                    <td>@item.Email</td>
                    <td>
                        <a href="@Url.Action("Student", new {item.HNo,item.Name,item.Family_Name,item.Address,item.City,item.Zip_code,item.Phone,item.Email})" class="btn btn-success">Edit</a>
                        <a href="@Url.Action("Delete", new {item.HNo})" class="btn btn-danger">Delete</a>

                    </td>
                </tr>
            }
</table>


